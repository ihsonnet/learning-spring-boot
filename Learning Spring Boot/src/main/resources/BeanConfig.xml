<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="coder1" class="com.learningspringboot.starterproject.model.Coder">
        <constructor-arg index="0" value="1001"/>
        <constructor-arg index="1" value="Md johny Morn"/>
        <constructor-arg index="2" value="Java"/>
        <constructor-arg index="3" ref="computer1"/>
    </bean>

    <bean id="computer1" class="com.learningspringboot.starterproject.model.Computer">
        <constructor-arg index="0" value="Dell"/>
    </bean>
</beans>